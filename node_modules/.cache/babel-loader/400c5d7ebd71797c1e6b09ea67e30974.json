{"ast":null,"code":"import circle from \"./circle.png\";\nimport x from \"./x.png\";\nexport default {\n  data() {\n    return {\n      id: 1,\n      isPlayer1: false,\n      img: \"\",\n      img1: circle,\n      img2: x\n    };\n  },\n\n  methods: {\n    clicked() {\n      this.isPlayer1 = !this.isPlayer1;\n    },\n\n    fillPlayerSign(i) {\n      if (this.isPlayer1) {\n        /*this.img = this.img1*/\n        this.img = this.img1;\n      } else {\n        this.img = this.img2;\n      }\n\n      let boxId = document.getElementById(i);\n      existingImg = boxId.attr('src');\n      let newImg = document.createElement(\"img\");\n      newImg.src = this.img;\n      newImg.style.width = '100%';\n      /*console.log(this.$refs.test)*/\n\n      boxId.appendChild(newImg);\n      boxId.src = this.img;\n      this.isPlayer1 = !this.isPlayer1;\n      console.log(this.isPlayer1);\n    }\n\n  },\n\n  mounted() {\n    console.log(document.getElementById(\"1\"));\n    console.log(\"test\");\n  }\n\n};","map":{"version":3,"mappings":"AAWI,OAAOA,MAAP,MAAmB,cAAnB;AACA,OAAOC,CAAP,MAAc,SAAd;AAEA,eAAe;EACXC,IAAG,GAAK;IACJ,OAAO;MACHC,EAAE,EAAE,CADD;MAEHC,SAAS,EAAE,KAFR;MAGHC,GAAG,EAAE,EAHF;MAIHC,IAAI,EAAEN,MAJH;MAKHO,IAAI,EAAEN;IALH,CAAP;EAQH,CAVU;;EAWXO,OAAO,EAAE;IACLC,OAAO,GAAG;MACN,KAAKL,SAAL,GAAiB,CAAC,KAAKA,SAAvB;IACH,CAHI;;IAILM,cAAc,CAACC,CAAD,EAAI;MACd,IAAI,KAAKP,SAAT,EAAoB;QAChB;QACA,KAAKC,GAAL,GAAW,KAAKC,IAAhB;MACJ,CAHA,MAGO;QACH,KAAKD,GAAL,GAAW,KAAKE,IAAhB;MACJ;;MAEA,IAAIK,KAAI,GAAIC,QAAQ,CAACC,cAAT,CAAwBH,CAAxB,CAAZ;MACAI,WAAU,GAAIH,KAAK,CAACI,IAAN,CAAW,KAAX,CAAd;MAGA,IAAIC,MAAK,GAAIJ,QAAQ,CAACK,aAAT,CAAuB,KAAvB,CAAb;MACAD,MAAM,CAACE,GAAP,GAAa,KAAKd,GAAlB;MACAY,MAAM,CAACG,KAAP,CAAaC,KAAb,GAAqB,MAArB;MAEA;;MAEAT,KAAK,CAACU,WAAN,CAAkBL,MAAlB;MACAL,KAAK,CAACO,GAAN,GAAY,KAAKd,GAAjB;MAEA,KAAKD,SAAL,GAAiB,CAAC,KAAKA,SAAvB;MACAmB,OAAO,CAACC,GAAR,CAAY,KAAKpB,SAAjB;IAEJ;;EA5BK,CAXE;;EA0CXqB,OAAM,GAAK;IACPF,OAAO,CAACC,GAAR,CAAYX,QAAQ,CAACC,cAAT,CAAwB,GAAxB,CAAZ;IACAS,OAAO,CAACC,GAA